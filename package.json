	"main": "index.js",
	"scripts": {
		"browsersync:reload": "browser-sync reload",
		"browsersync:start": "browser-sync start --proxy localhost:8080",
		"build": "amagaki build",
		"dev": "npm-run-all --parallel watch:src watch:pages start browsersync:start",
		"start": "amagaki serve",
		"watch:src": "nodemon --watch src -e js,css --exec npm run browsersync:reload",
		"watch:pages": "nodemon --watch content --watch views -e njk,yaml --exec npm run browsersync:reload"
	},
